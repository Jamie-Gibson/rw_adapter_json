{
    "id": "rw_adapter_json_v1",
    "name": "RW Adapter Json v1",
    "urls": [{
        "url": "/query/:dataset",
        "method": "GET",
        "paramProvider": "dataset",
        "dataProvider": "dataset",
        "filters":{
            "provider": "RwJson"
        },
        "endpoints": [{
            "method": "POST",
            "baseUrl": "http://localhost:3010",
            "path": "/query/:dataset",
            "data": ["dataset"]
        }]
    }]
}
